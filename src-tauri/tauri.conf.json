{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "aitextaudioo",
  "version": "0.1.0",
  "identifier": "com.example.aitextaudioo",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:5173",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Voice-to-Text",
        "width": 900,
        "height": 700,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all"
  }
}

